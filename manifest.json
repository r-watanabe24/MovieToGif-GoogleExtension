{
    "manifest_version": 3,
    "name": "Video â†’ GIF Converter",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "downloads"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/gifjs/gif.worker.js"
            ],
            "matches": [
                "<all_urls>"
